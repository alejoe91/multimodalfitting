    "distributions": {
        "exp": {
            "fun": "(-0.8696 + 2.087*math.exp(({distance})*0.0031))*{value}",
            "soma_ref_point": 0.5,
            "__comment": "distribution based on Kole et al. 2006"
        },
        "decay": {
            "fun": "math.exp({distance}*{constant_decay})*{value}",
            "soma_ref_point": 0.5,
            "parameters": [
                "constant_decay"
            ]
        },
        "sig_increase": {
            "fun": "(1-(1/(1+math.exp(({distance}-20)/2.5))))*{value}",
            "ref_point": 0.0,
            "ref_section": "ais[0]",
            "__comment": "sigmoidal increase based on increase of Nav1.6 in AIS, Hu et al., 2009"
        },
        "sig_decrease": {
            "fun": "(1/(1+math.exp(({distance}-25)/3)))*{value}",
            "ref_point": 0.0,
            "ref_section": "ais[0]",
            "__comment": "sigmoidal decrease based on decrease of Nav1.2 in AIS, Hu et al., 2009"
        }
    },
    "parameters": {
        "__comment": "define constants as single values and params to optimize as tuples of bounds: [lower, upper]",
